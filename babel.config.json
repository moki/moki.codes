{
        "presets": [
                [
                        "@babel/preset-env",
                        {
                                "targets": "> 0.25%, not dead"
                        }
                ], [
                        "@babel/preset-typescript",
                        {
                                "isTSX": true,
                                "allExtensions": true,
                                "jsxPragma": "h"
                        }
                ]
        ],
        "plugins": [
                [
                        "@babel/plugin-transform-react-jsx",
                        {
                                "pragma": "h",
                                "pragmaFrag": "f"
                        }
                ],
                [
                        "@babel/plugin-transform-runtime",
                        {
                                "regenerator": true
                        }
                ]
        ]
}
